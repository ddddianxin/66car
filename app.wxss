}

.h2Title{
  font-size: 14px;
  color:#fff;
  border-left:5px solid #ffd98d;
  padding-left: 10px;
  font-size: bold;
  margin-bottom:15px;
} 



/* 车型列表 */
.item{
  background-color: #1a1a1c;
  height: 80px;
  padding:0 15px;
  display: flex;
  flex-wrap: nowrap;
  margin-bottom: 1px;
  justify-content: space-around;
}
.img{
  width:400rpx;
  height: 100%;
}
.info{
  padding:5px 0 5px 15px;
  width: 100%;
}
.tit{
  color:#fff;
  font-size: 14px;
  display: block;
  margin-bottom:5px;
  max-width: 400rpx;
}
.keyword{
  font-size: 10px;
  display: flex;
  flex-wrap: nowrap;
  margin-bottom:5px;
}
.k_text{
  color:#999;
  background-color:#212026;
  padding:3px 5px;
  margin-right:5px;
  border-radius: 3px;
}
.price{
  text-align: right;
  color:#ffd98d;
  width:100%;
  font-size: 14px;
}