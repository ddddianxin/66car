/* 产品展示 */
.good-box{
  display:flex;
  flex-wrap:nowrap;
  padding:10px 0;
  margin:0 15px;
  border-bottom:1px solid #d6d6db;
}

.good-box:last-child{
  border-bottom:none;
}

.pic{
  width:60px;
  background-color: #fff;
}

.pic image{
  width:60px;
  height:60px;
}

.info{
  width:78%;
  display:block;
  padding-left:15px;
  font-size: 0;
}

.ch-tit{
  font-size:14px;
  line-height: 20px;
}
.en-tit{
  font-size:12px;
  color:#999;
  margin:3px 0 5px 0;
  width:100%;
  
}
.cas{
  background-color:#0e79cc;
  color:#fff;
  font-size:12px;
  padding:0px 10px 0px 5px;
  border-radius:0 10px 10px 0;
  height:20px;
  line-height: 20px;
  display: inline-block;
}

.arrow{
  width:8px;
  height: 15px;
}


.noData{
  min-height:500px;
